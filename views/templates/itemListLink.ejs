<%

itemListLink_Data.items.forEach(function(item) {

%>
<div class="row">
    <div class="col-md-12">
        <div class = "list-group">
            <a href = "<%= item.url %>" class = "list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-2" style="text-align: -webkit-center;">
                                    <img src="<%= item.imageURL %>" />
                                </div>
                                <div class="col-md-10">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h3>
                                                <%= item.title %>
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <p>
                                                <%= item.detail1 %>
                                            </p>
                                        </div>
                                        <div class="col-md-4">
                                            <p>
                                                <%= item.detail2 %>
                                            </p>
                                        </div>
                                        <div class="col-md-4">
                                            <p>
                                                <%= item.detail3 %>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    </div>
</div>

<%
    });
%>